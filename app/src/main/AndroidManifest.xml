<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inlocker_shopkeeper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".SellPageActivity" />
        <activity android:name=".SellerViewActivity" />
        <activity android:name=".FinishedOrderActivity" />
        <activity android:name=".SetStoreInfoActivity" />
        <activity
            android:name=".NewUploadItemActivity"
            android:theme="@style/customDialog" />
        <activity android:name=".EditCategoriesActivity" />
    </application>

</manifest>